{
	"expo": {
		"name": "shopping-heaven",
		"slug": "shopping-heaven",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "cover",
			"backgroundColor": "#ffffff"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": [
			"**/*"
		],
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.satishnaikawadi2001.shoppingheaven"
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#FFFFFF"
			},
			"package": "com.satishnaikawadi2001.shoppingheaven",
			"permissions": [
				"ACCESS_BACKGROUND_LOCATION"
			]
		},
		"web": {
			"favicon": "./assets/favicon.png"
		}
	}
}
